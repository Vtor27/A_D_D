<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="es.florida">
	<class name="Coche" table="coches">
		<id name="id" column="id">
			<generator class="increment" />
		</id>
		<property name="año" type="int" column="año" />
		<property name="marca" type="string" column="marca" />
		<property name="modelo" type="string" column="modelo" />
		<property name="color" type="string" column="color" />
	</class>
</hibernate-mapping>